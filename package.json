{
    "name": "nmmes-cli",
    "version": "0.0.3",
    "description": "A command line interface front end for nmmes-backend.",
    "author": "Ayrton Sparling",
    "license": "LGPL-3.0",
    "engines": {
        "node": ">=9.0.0"
    },
    "bin": {
        "nmmes": "dist/nmmes-cli.js"
    },
    "scripts": {
        "prepare": "npm run dist",
        "test": "npm run dist && universalis-tester ./test/index.js",
        "dev": "webpack-cli --mode=development --watch",
        "dist": "webpack-cli --mode=production --progress",
        "build": "npm run dist && npm run pack",
        "pack": "pkg -t latest-linux-x64,latest-win-x64 ./dist/package.json --out-path build",
        "profile": "node --prof-process isolate-0xnnnnnnnnnnnn-v8.log > processed.txt",
        "postversion": "git push"
    },
    "main": "dist/nmmes-cli.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/NMMES/nmmes-cli.git"
    },
    "dependencies": {
        "chalk": "^2.3.2",
        "chokidar": "^2.0.3",
        "fs-extra": "^5.0.0",
        "git-rev-sync": "^1.9.1",
        "is-url": "^1.2.3",
        "mime": "^2.0.3",
        "nmmes-backend": "0.0.4",
        "node-fetch": "^2.1.1",
        "recursive-readdir": "^2.2.2",
        "requireg": "^0.1.7",
        "source-map-support": "^0.5.4",
        "yargs": "^11.0.0"
    },
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.2.2",
        "babel-loader": "^7.1.4",
        "babel-plugin-lodash": "^3.2.11",
        "babel-plugin-module-resolver": "^3.1.0",
        "babel-preset-es2016": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "babel-register": "^6.26.0",
        "babel-runtime": "^6.26.0",
        "eslint": "^4.19.1",
        "must": "^0.13.4",
        "pkg": "^4.2.5",
        "source-map-loader": "^0.2.2",
        "webpack": "^4.2.0",
        "webpack-cli": "^2.0.13",
        "webpack-node-externals": "^1.6.0"
    }
}
